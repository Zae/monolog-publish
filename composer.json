{
    "name": "zae/monolog-publish",
    "description": "A publisher handler for monolog",
    "keywords": ["monolog", "publish", "redis"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Ezra Pool",
            "email": "ezra@tsdme.nl"
        }
    ],
    "require": {
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "predis/predis": "~1.0",
        "mockery/mockery": "~0.9"
    },
    "suggest": {
        "predis/predis": "Needed to use the RedisPublisher"
    },
    "autoload": {
        "psr-4": {
            "Zae\\Monolog\\Publish\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "phpunit"
    }
}

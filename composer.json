{
    "name": "zae/monolog-publish",
    "description": "A publisher handler for monolog",
    "keywords": ["monolog", "publish", "zae"],
    "license": "MIT",
    "type": "library",
    "require": {
        "monolog/monolog": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "predis/predis": "^1.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Zae\\Monolog\\Publish\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
